generator client {
  provider = "prisma-client-js"
}

datasource db {
  //provider = "mysql"
  provider = "postgres"
  url      = env("DATABASE_URL")
}

model user {
  id       Int    @id @default(autoincrement())
  name     String @db.VarChar(45)
  email    String @unique(map: "email_UNIQUE") @db.VarChar(45)
  password String @db.VarChar(45)
}
